{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "style:all": "npm run style:lint && npm run style:prettier",
    "style:lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "style:prettier": "prettier --check . --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx",
    "style:fix": "prettier --write . --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "next": "13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "prettier": "2.7.1",
    "tailwindcss": "^3.2.3"
  }
}
